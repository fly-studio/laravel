{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "addons/core": "^7.0",
        "addons/elasticsearch": "^3.0",
        "addons/entrust": "^2.0",
        "addons/plugins": "^1.0",
        "addons/server": "dev-master",
        "doctrine/dbal": "^2.9",
        "endroid/qr-code": "^3.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "geoip2/geoip2": "^2.9",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "jenssegers/mongodb": "dev-develop",
        "laravel/framework": "^7.0",
        "laravel/passport": "^8.0",
        "laravel/scout": "^8.0",
        "laravel/tinker": "^2.0",
        "lokielse/omnipay-alipay": "^3.0",
        "lokielse/omnipay-wechatpay": "^3.0",
        "malkusch/lock": "^2.1",
        "mews/captcha": "^3.0",
        "overtrue/wechat": "^4.2",
        "owen-it/laravel-auditing": "^10.0",
        "phpoffice/phpspreadsheet": "^1.6",
        "phpseclib/phpseclib": "^2.0",
        "predis/predis": "^1.1",
        "socialiteproviders/qq": "^3.0",
        "socialiteproviders/weixin-web": "^3.0",
        "symfony/dom-crawler": "^5.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "beyondcode/laravel-dump-server": "^1.4",
        "barryvdh/laravel-debugbar": "^3.2",
        "rap2hpoutre/laravel-log-viewer": "^1.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "symfony/thanks": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "classmap": [

        ],
        "psr-4": {

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [

        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }

}
