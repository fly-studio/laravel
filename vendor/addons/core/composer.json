{
    "name": "addons/core",
    "description": "Laravel's Addons.",
    "keywords": ["L5", "Laravel 5", "Laravel","Addons"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fly",
            "email": "fly@load-page.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "illuminate/cache": "5.*",
        "illuminate/config": "5.*",
        "smarty/smarty": "3.*@stable"
        "addons/smarty": "~1.*"
    },
    "autoload": {
        "psr-4": {
            "Addons\\Core\\": "src/"
        },
        "classmap": [
            "/src"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}